<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml>

<?xdevuiml version="1.0"?>
<?template NONE?>
<?import com.xdev.ui.XdevButton?>
<?import com.xdev.ui.XdevGridLayout?>
<?import com.xdev.ui.XdevHorizontalLayout?>
<?import com.xdev.ui.XdevLabel?>
<?import com.xdev.ui.XdevPopupDateField?>
<?import com.xdev.ui.XdevView?>
<XdevView xmlns:x="http://xml.xdev-software.com/xdevuiml" x:name="ResetPayDatePopup">
    <x:constraints zpos="0" width="100%" height="100%" />
    <XdevGridLayout x:name="gridLayout">
        <x:constraints zpos="0" width="100%" height="100%" />
        <XdevLabel x:name="label2" value="Buchungsdatum zurück setzen.">
            <x:constraints gridx="1" gridy="1" top="20px" left="10px" weighty="0.0" weightx="0.0" width="100%" gridWidth="2" />
        </XdevLabel>
        <XdevLabel x:name="label" value="Lohndatum per">
            <x:constraints gridx="1" gridy="2" top="68px" left="10px" weighty="0.0" weightx="0.0" />
        </XdevLabel>
        <XdevPopupDateField description="Admin" required="true" x:name="popupDateField">
            <x:constraints gridx="2" gridy="2" top="62px" left="118px" weighty="0.0" weightx="0.0" />
        </XdevPopupDateField>
        <XdevHorizontalLayout x:name="horizontalLayout">
            <x:constraints gridx="1" gridy="3" weighty="0.0" weightx="0.0" width="100%" alignment="MIDDLE_CENTER" gridWidth="2" />
            <XdevButton caption="Zurück setzen" x:name="btnCreate">
                <x:constraints gridHeight="1" gridx="1" gridy="3" top="128px" left="86px" weighty="0.0" weightx="0.0" gridWidth="1" />
                <x:event>click.buttonClick</x:event>
            </XdevButton>
            <XdevButton icon="WebContent/WEB-INF/resources/images/delete1.png" caption="Abbrechen" x:name="btnCancel">
                <x:constraints gridHeight="1" gridx="2" gridy="3" top="128px" left="210px" weighty="0.0" weightx="0.0" gridWidth="1" />
                <x:event>click.buttonClick</x:event>
            </XdevButton>
        </XdevHorizontalLayout>
    </XdevGridLayout>
</XdevView>

